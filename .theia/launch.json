{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
      
    {
        "name": "Python: Flask",
        "type": "python",
        "request": "launch",
        "module": "flask",
        "cwd": "${workspaceFolder}/web",
        "env": {
            "FLASK_APP": "main.py",
            "FLASK_ENV": "development",
            "FLASK_DEBUG": "0"
        },
        "jinja": true,
        "console": "integratedTerminal"
    },
    {
        "name": "Python: test",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceFolder}/web",
        "program": "main.py",
        "jinja": true,
        "console": "integratedTerminal"
    },
  ]
}
